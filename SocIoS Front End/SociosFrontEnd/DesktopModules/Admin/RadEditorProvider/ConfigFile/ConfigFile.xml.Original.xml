<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <property name="Common Settings" IsSeparator="true" />
    <property name="Skin">Default</property>
    <property name="AutoResizeHeight">true</property>
    <property name="BorderWidth">0</property>
    <property name="Height">500px</property>
    <property name="Width">680px</property>
    <property name="EditModes">All</property>
    <property name="EnableResize">true</property>
    <property name="Language"></property>
    <property name="ContentFilters">DefaultFilters</property>
    <property name="CssFile">editor.css</property>
    <property name="NewLineBr">False</property>
    <property name="StripFormattingOptions">MSWordRemoveAll</property>
    <property name="LinksUseRelativeUrls">True</property>
    <property name="LinksUseTabNames">false</property>
    <property name="ShowPortalLinks">True</property>
    <property name="ContentAreaMode"></property>

    <property name="Toolbar Settings" IsSeparator="true" />
    <property name="ToolbarMode">Default</property>
    <property name="ToolsWidth"></property>

    <property name="DMX Integration Settings" IsSeparator="true" />
    <property name="DMXIntegration">False</property>
    <property name="DMXPath"></property>

    <property name="Document Manager Settings" IsSeparator="true" />
    <property name="DocumentsFilters">*.*</property>
    <property name="DocumentsPath">/Documents</property>
    <property name="MaxDocumentSize">1024000</property>

    <property name="Image Manager Settings" IsSeparator="true" />
    <property name="ImagesPath">/Images</property>
    <property name="MaxImageSize">1024000</property>
    <property name="ImagesFilters">
        <item>*.jpg</item>
        <item>*.png</item>
        <item>*.jpeg</item>
        <item>*.bmp</item>
        <item>*.gif</item>
    </property>

    <property name="Flash Manager Settings" IsSeparator="true" />
    <property name="FlashPath">/Flash</property>
    <property name="MaxFlashSize">1024000</property>
    <property name="FlashFilters">
        <item>*.swf</item>
    </property>

    <property name="Media Manager Settings" IsSeparator="true" />
    <property name="MediaPath">/Media</property>
    <property name="MaxMediaSize">1024000</property>
    <property name="MediaFilters">
        <item>*.avi</item>
        <item>*.mpg</item>
    </property>

    <property name="Silverlight Manager Settings" IsSeparator="true" />
    <property name="SilverlightPath">/Silverlight</property>
    <property name="SilverlightFilters">.*</property>   
    <property name="MaxSilverlightSize">1024000</property>

    <property name="Template Manager Settings" IsSeparator="true" />
    <property name="TemplatePath">/Templates</property>
    <property name="MaxTemplateSize">1024000</property>
    <property name="TemplateFilters">
        <item>*.html</item>
        <item>*.htm</item>
    </property>


    <property name="Client Script Settings" IsSeparator="true" />       
    <property name="OnClientCommandExecuting"></property>
    <property name="OnClientPasteHtml"></property>
    <property name="ScriptToLoad"></property>



    <property name="Developer Settings" IsSeparator="true" />
    <property name="FileBrowserContentProviderTypeName">DotNetNuke.Providers.RadEditorProvider.TelerikFileBrowserProvider, DotNetNuke.RadEditorProvider</property>
    <property name="SpellAllowAddCustom"></property>
    <property name="SpellCheckProvider"></property>
    <property name="SpellCustomDictionarySourceTypeName"></property>
    <property name="SpellCustomDictionarySuffix"></property>
    <property name="SpellDictionaryLanguage"></property>
    <property name="SpellDictionaryPath"></property>
    <property name="SpellEditDistance"></property>
    <property name="SpellFragmentIgnoreOptions"></property>
    <property name="SpellWordIgnoreOptions"></property>    
       
</configuration>