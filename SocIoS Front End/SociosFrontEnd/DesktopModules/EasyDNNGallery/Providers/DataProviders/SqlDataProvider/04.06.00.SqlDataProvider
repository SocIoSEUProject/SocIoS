IF NOT EXISTS (SELECT * FROM dbo.sysobjects WHERE id = OBJECT_ID(N'[dbo].[EasyGalleryInfo]') AND OBJECTPROPERTY(id, N'IsUserTable') = 1)
BEGIN
	SET ANSI_NULLS ON
	SET QUOTED_IDENTIFIER ON
	CREATE TABLE [dbo].[EasyGalleryInfo](
		[EntryID] [int] IDENTITY(1,1) NOT NULL,
		[Info1] [nvarchar](300) NOT NULL,
		[Info2] [nvarchar](300) NOT NULL,
	 CONSTRAINT [PK_EasyGalleryInfoPrimary] PRIMARY KEY CLUSTERED ([EntryID] ASC) WITH (STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF)
	)
END